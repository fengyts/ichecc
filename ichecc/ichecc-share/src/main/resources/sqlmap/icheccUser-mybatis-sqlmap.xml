<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ichecc.domain.IcheccUserMapper">

	<resultMap type="com.ichecc.domain.IcheccUserDO" id="IcheccUserDOResultMap">
		<id column="id" property="id" />
        <result column="openid" property="openid"/>
        <result column="nickname" property="nickname"/>
        <result column="mobile" property="mobile"/>
        <result column="password" property="password"/>
        <result column="salt" property="salt"/>
        <result column="is_certification" property="isCertification"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_time" property="modifyTime"/>
        <result column="last_login_time" property="lastLoginTime"/>
	</resultMap>

	<sql id="MybatisIcheccUserDAO_all_column_fields">
	      id,openid,nickname,mobile,password,salt,is_certification,create_time,modify_time,last_login_time
 	</sql>
	<!-- 只有varchar char text类型 做 !='' 判断，生成代码时,其他类型时只做!=null 决断 0或fasle与空''是相同的，在mybatis中的动态语句中 -->
	<sql id="MybatisIcheccUserDAO_dynamic_where_fields"><!-- xml转义字符需要 <![CDATA[   ]]> 标签-->
		<where>
			<if test="openid != null  and openid != '' "> AND openid=#{openid} </if>
			<if test="nickname != null  and nickname != '' "> AND nickname=#{nickname} </if>
			<if test="mobile != null  and mobile != '' "> AND mobile=#{mobile} </if>
			<if test="password != null  and password != '' "> AND password=#{password} </if>
			<if test="salt != null  and salt != '' "> AND salt=#{salt} </if>
			<if test="isCertification != null "> AND is_certification=#{isCertification} </if>
		</where>
 	</sql>
	
	<select id="MybatisIcheccUserDAO_selectById" parameterType="long" resultMap="IcheccUserDOResultMap">
		SELECT
			<include refid="MybatisIcheccUserDAO_all_column_fields" />
		FROM
			ichecc_user
		WHERE
			 id = #{id}
	</select>
	
	
	
    <insert id="MybatisIcheccUserDAO_insert" parameterType="com.ichecc.domain.IcheccUserDO" useGeneratedKeys="true">
        <selectKey resultType="long" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID() AS id
        </selectKey>
        INSERT INTO ichecc_user(
				id,
				openid,
				nickname,
				mobile,
				password,
				salt,
				is_certification,
				create_time,
				modify_time,
				last_login_time
		)values(
				#{id},
				#{openid},
				#{nickname},
				#{mobile},
				#{password},
				#{salt},
				#{isCertification},
				#{createTime},
				#{modifyTime},
				#{lastLoginTime}
		)
    </insert>
	

	<update id="MybatisIcheccUserDAO_updateById" parameterType="com.ichecc.domain.IcheccUserDO">
		UPDATE ichecc_user
		SET
					openid = #{openid},
					nickname = #{nickname},
					mobile = #{mobile},
					password = #{password},
					salt = #{salt},
					is_certification = #{isCertification},
					modify_time = #{modifyTime},
					last_login_time = #{lastLoginTime}
		WHERE
			id = #{id}
	</update>

	<delete id="MybatisIcheccUserDAO_deleteById" parameterType="long">
		DELETE FROM ichecc_user WHERE id = #{id}
	</delete>
	
	
	<update id="MybatisIcheccUserDAO_update_dynamic" parameterType="com.ichecc.domain.IcheccUserDO">
		UPDATE ichecc_user
		<set>
					<if test="openid != null  and openid != '' ">openid=#{openid},</if>
					<if test="nickname != null  and nickname != '' ">nickname=#{nickname},</if>
					<if test="mobile != null  and mobile != '' ">mobile=#{mobile},</if>
					<if test="password != null  and password != '' ">password=#{password},</if>
					<if test="salt != null  and salt != '' ">salt=#{salt},</if>
					<if test="isCertification != null ">is_certification=#{isCertification},</if>
					<if test="modifyTime != null ">modify_time=#{modifyTime},</if>
					<if test="lastLoginTime != null ">last_login_time=#{lastLoginTime},</if>
		</set>
		WHERE id = #{id}
	</update>

 	<select id="MybatisIcheccUserDAO_select_dynamic" resultMap="IcheccUserDOResultMap" parameterType="com.ichecc.domain.IcheccUserDO">
	     SELECT
	     	<include refid="MybatisIcheccUserDAO_all_column_fields" />
		 FROM
		 	ichecc_user
			<include refid="MybatisIcheccUserDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisIcheccUserDAO_select_dynamic_count" resultType="long" parameterType="com.ichecc.domain.IcheccUserDO">
	     SELECT
	     	count(1)
		 FROM
		 	ichecc_user
	 	<include refid="MybatisIcheccUserDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisIcheccUserDAO_select_dynamic_page_query" resultMap="IcheccUserDOResultMap" parameterType="com.ichecc.domain.IcheccUserDO">
	     SELECT
	     	<include refid="MybatisIcheccUserDAO_all_column_fields" />
		 FROM
		 	ichecc_user
	 	<include refid="MybatisIcheccUserDAO_dynamic_where_fields" />
	 	ORDER BY ichecc_user.id DESC
	 	Limit #{start},#{pageSize}
 	</select>

</mapper>