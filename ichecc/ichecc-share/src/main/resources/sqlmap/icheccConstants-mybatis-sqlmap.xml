<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ichecc.domain.IcheccConstantsMapper">

	<resultMap type="com.ichecc.domain.IcheccConstantsDO" id="IcheccConstantsDOResultMap">
		<id column="key" property="key" />
        <result column="value" property="value"/>
        <result column="desc" property="desc"/>
        <result column="create_user_id" property="createUserId"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_user_id" property="modifyUserId"/>
        <result column="modify_time" property="modifyTime"/>
	</resultMap>

	<sql id="MybatisIcheccConstantsDAO_all_column_fields">
	      key,value,desc,create_user_id,create_time,modify_user_id,modify_time
 	</sql>
	<!-- 只有varchar char text类型 做 !='' 判断，生成代码时,其他类型时只做!=null 决断 0或fasle与空''是相同的，在mybatis中的动态语句中 -->
	<sql id="MybatisIcheccConstantsDAO_dynamic_where_fields"><!-- xml转义字符需要 <![CDATA[   ]]> 标签-->
		<where>
			<if test="value != null  and value != '' "> AND value=#{value} </if>
			<if test="desc != null  and desc != '' "> AND desc=#{desc} </if>
			<if test="createUserId != null "> AND create_user_id=#{createUserId} </if>
			<if test="modifyUserId != null "> AND modify_user_id=#{modifyUserId} </if>
		</where>
 	</sql>
	
	<select id="MybatisIcheccConstantsDAO_selectById" parameterType="long" resultMap="IcheccConstantsDOResultMap">
		SELECT
			<include refid="MybatisIcheccConstantsDAO_all_column_fields" />
		FROM
			ichecc_constants
		WHERE
			 key = #{key}
	</select>
	
	<select id="MybatisIcheccConstantsDAO_selectByPrimaryKey" parameterType="string" resultMap="IcheccConstantsDOResultMap">
		SELECT
			<include refid="MybatisIcheccConstantsDAO_all_column_fields" />
		FROM
			ichecc_constants
		WHERE
			 key = #{key}
	</select>
	
	
    <insert id="MybatisIcheccConstantsDAO_insert" parameterType="com.ichecc.domain.IcheccConstantsDO" useGeneratedKeys="true">
        <selectKey resultType="long" keyProperty="key" order="AFTER">
            SELECT LAST_INSERT_ID() AS key
        </selectKey>
        INSERT INTO ichecc_constants(
				key,
				value,
				desc,
				create_user_id,
				create_time,
				modify_user_id,
				modify_time
		)values(
				#{key},
				#{value},
				#{desc},
				#{createUserId},
				#{createTime},
				#{modifyUserId},
				#{modifyTime}
		)
    </insert>
	
	<update id="MybatisIcheccConstantsDAO_updateByPrimaryKey" parameterType="com.ichecc.domain.IcheccConstantsDO">
		UPDATE ichecc_constants
		SET
					value = #{value},
					desc = #{desc},
					create_user_id = #{createUserId},
					modify_user_id = #{modifyUserId},
					modify_time = #{modifyTime}
		WHERE
			key = #{key}
	</update>

	<delete id="MybatisIcheccConstantsDAO_deleteByPrimaryKey" parameterType="string">
		DELETE FROM ichecc_constants WHERE key = #{key}
	</delete>

	<update id="MybatisIcheccConstantsDAO_updateById" parameterType="com.ichecc.domain.IcheccConstantsDO">
		UPDATE ichecc_constants
		SET
					value = #{value},
					desc = #{desc},
					create_user_id = #{createUserId},
					modify_user_id = #{modifyUserId},
					modify_time = #{modifyTime}
		WHERE
			key = #{key}
	</update>

	<delete id="MybatisIcheccConstantsDAO_deleteById" parameterType="long">
		DELETE FROM ichecc_constants WHERE key = #{key}
	</delete>
	
	
	<update id="MybatisIcheccConstantsDAO_update_dynamic" parameterType="com.ichecc.domain.IcheccConstantsDO">
		UPDATE ichecc_constants
		<set>
					<if test="value != null  and value != '' ">value=#{value},</if>
					<if test="desc != null  and desc != '' ">desc=#{desc},</if>
					<if test="createUserId != null ">create_user_id=#{createUserId},</if>
					<if test="modifyUserId != null ">modify_user_id=#{modifyUserId},</if>
					<if test="modifyTime != null ">modify_time=#{modifyTime},</if>
		</set>
		WHERE key = #{key}
	</update>

 	<select id="MybatisIcheccConstantsDAO_select_dynamic" resultMap="IcheccConstantsDOResultMap" parameterType="com.ichecc.domain.IcheccConstantsDO">
	     SELECT
	     	<include refid="MybatisIcheccConstantsDAO_all_column_fields" />
		 FROM
		 	ichecc_constants
			<include refid="MybatisIcheccConstantsDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisIcheccConstantsDAO_select_dynamic_count" resultType="long" parameterType="com.ichecc.domain.IcheccConstantsDO">
	     SELECT
	     	count(1)
		 FROM
		 	ichecc_constants
	 	<include refid="MybatisIcheccConstantsDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisIcheccConstantsDAO_select_dynamic_page_query" resultMap="IcheccConstantsDOResultMap" parameterType="com.ichecc.domain.IcheccConstantsDO">
	     SELECT
	     	<include refid="MybatisIcheccConstantsDAO_all_column_fields" />
		 FROM
		 	ichecc_constants
	 	<include refid="MybatisIcheccConstantsDAO_dynamic_where_fields" />
	 	ORDER BY ichecc_constants.id DESC
	 	Limit #{start},#{pageSize}
 	</select>

</mapper>