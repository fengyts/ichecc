<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ichecc.domain.VipDepositConfigMapper">

	<resultMap type="com.ichecc.domain.VipDepositConfigDO" id="VipDepositConfigDOResultMap">
		<id column="id" property="id" />
        <result column="amount" property="amount"/>
        <result column="discount" property="discount"/>
        <result column="expiry_date" property="expiryDate"/>
        <result column="expiry_type" property="expiryType"/>
        <result column="status" property="status"/>
        <result column="create_user_id" property="createUserId"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_user_id" property="modifyUserId"/>
        <result column="modify_time" property="modifyTime"/>
	</resultMap>

	<sql id="MybatisVipDepositConfigDAO_all_column_fields">
	      id,amount,discount,expiry_date,expiry_type,status,create_user_id,create_time,modify_user_id,modify_time
 	</sql>
	<!-- 只有varchar char text类型 做 !='' 判断，生成代码时,其他类型时只做!=null 决断 0或fasle与空''是相同的，在mybatis中的动态语句中 -->
	<sql id="MybatisVipDepositConfigDAO_dynamic_where_fields"><!-- xml转义字符需要 <![CDATA[   ]]> 标签-->
		<where>
			<if test="amount != null "> AND amount=#{amount} </if>
			<if test="discount != null "> AND discount=#{discount} </if>
			<if test="expiryDate != null "> AND expiry_date=#{expiryDate} </if>
			<if test="expiryType != null  and expiryType != '' "> AND expiry_type=#{expiryType} </if>
			<if test="status != null "> AND status=#{status} </if>
			<if test="createUserId != null "> AND create_user_id=#{createUserId} </if>
			<if test="modifyUserId != null "> AND modify_user_id=#{modifyUserId} </if>
		</where>
 	</sql>
	
	<select id="MybatisVipDepositConfigDAO_selectById" parameterType="long" resultMap="VipDepositConfigDOResultMap">
		SELECT
			<include refid="MybatisVipDepositConfigDAO_all_column_fields" />
		FROM
			vip_deposit_config
		WHERE
			 id = #{id}
	</select>
	
	
	
    <insert id="MybatisVipDepositConfigDAO_insert" parameterType="com.ichecc.domain.VipDepositConfigDO" useGeneratedKeys="true">
        <selectKey resultType="long" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID() AS id
        </selectKey>
        INSERT INTO vip_deposit_config(
			id,
			amount,
			discount,
			expiry_date,
			expiry_type,
			status,
			create_user_id,
			create_time,
			modify_user_id,
			modify_time
		)values(
			#{id},
			#{amount},
			#{discount},
			#{expiryDate},
			#{expiryType},
			#{status},
			#{createUserId},
			#{createTime},
			#{modifyUserId},
			#{modifyTime}
		)
    </insert>
	

	<update id="MybatisVipDepositConfigDAO_updateById" parameterType="com.ichecc.domain.VipDepositConfigDO">
		UPDATE vip_deposit_config
		SET
			amount = #{amount},
			discount = #{discount},
			expiry_date = #{expiryDate},
			expiry_type = #{expiryType},
			status = #{status},
			create_user_id = #{createUserId},
			modify_user_id = #{modifyUserId},
			modify_time = #{modifyTime}
		WHERE
			id = #{id}
	</update>

	<delete id="MybatisVipDepositConfigDAO_deleteById" parameterType="long">
		DELETE FROM vip_deposit_config WHERE id = #{id}
	</delete>
	
	
	<update id="MybatisVipDepositConfigDAO_update_dynamic" parameterType="com.ichecc.domain.VipDepositConfigDO">
		UPDATE vip_deposit_config
		<set>
			<if test="amount != null ">amount=#{amount},</if>
			<if test="discount != null ">discount=#{discount},</if>
			<if test="expiryDate != null ">expiry_date=#{expiryDate},</if>
			<if test="expiryType != null  and expiryType != '' ">expiry_type=#{expiryType},</if>
			<if test="status != null ">status=#{status},</if>
			<if test="createUserId != null ">create_user_id=#{createUserId},</if>
			<if test="modifyUserId != null ">modify_user_id=#{modifyUserId},</if>
			<if test="modifyTime != null ">modify_time=#{modifyTime},</if>
		</set>
		WHERE id = #{id}
	</update>

 	<select id="MybatisVipDepositConfigDAO_select_dynamic" resultMap="VipDepositConfigDOResultMap" parameterType="com.ichecc.domain.VipDepositConfigDO">
	     SELECT
	     	<include refid="MybatisVipDepositConfigDAO_all_column_fields" />
		 FROM
		 	vip_deposit_config
			<include refid="MybatisVipDepositConfigDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisVipDepositConfigDAO_select_dynamic_count" resultType="long" parameterType="com.ichecc.domain.VipDepositConfigDO">
	     SELECT
	     	count(1)
		 FROM
		 	vip_deposit_config
	 	<include refid="MybatisVipDepositConfigDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisVipDepositConfigDAO_select_dynamic_page_query" resultMap="VipDepositConfigDOResultMap" parameterType="com.ichecc.domain.VipDepositConfigDO">
	     SELECT
	     	<include refid="MybatisVipDepositConfigDAO_all_column_fields" />
		 FROM
		 	vip_deposit_config
	 	<include refid="MybatisVipDepositConfigDAO_dynamic_where_fields" />
	 	ORDER BY vip_deposit_config.id DESC
	 	Limit #{start},#{pageSize}
 	</select>

</mapper>