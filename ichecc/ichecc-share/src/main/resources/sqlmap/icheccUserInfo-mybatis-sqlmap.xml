<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ichecc.domain.IcheccUserInfoMapper">

	<resultMap type="com.ichecc.domain.IcheccUserInfoDO" id="IcheccUserInfoDOResultMap">
		<id column="user_id" property="userId" />
        <result column="name" property="name"/>
        <result column="identity_card" property="identityCard"/>
        <result column="address" property="address"/>
        <result column="province" property="province"/>
        <result column="city" property="city"/>
        <result column="country" property="country"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_time" property="modifyTime"/>
	</resultMap>

	<sql id="MybatisIcheccUserInfoDAO_all_column_fields">
	      user_id,name,identity_card,address,province,city,country,create_time,modify_time
 	</sql>
	<!-- 只有varchar char text类型 做 !='' 判断，生成代码时,其他类型时只做!=null 决断 0或fasle与空''是相同的，在mybatis中的动态语句中 -->
	<sql id="MybatisIcheccUserInfoDAO_dynamic_where_fields"><!-- xml转义字符需要 <![CDATA[   ]]> 标签-->
		<where>
			<if test="name != null  and name != '' "> AND name=#{name} </if>
			<if test="identityCard != null  and identityCard != '' "> AND identity_card=#{identityCard} </if>
			<if test="address != null  and address != '' "> AND address=#{address} </if>
			<if test="province != null  and province != '' "> AND province=#{province} </if>
			<if test="city != null  and city != '' "> AND city=#{city} </if>
			<if test="country != null  and country != '' "> AND country=#{country} </if>
		</where>
 	</sql>
	
	<select id="MybatisIcheccUserInfoDAO_selectById" parameterType="long" resultMap="IcheccUserInfoDOResultMap">
		SELECT
			<include refid="MybatisIcheccUserInfoDAO_all_column_fields" />
		FROM
			ichecc_user_info
		WHERE
			 user_id = #{userId}
	</select>
	
	
	
    <insert id="MybatisIcheccUserInfoDAO_insert" parameterType="com.ichecc.domain.IcheccUserInfoDO" useGeneratedKeys="true">
        <selectKey resultType="long" keyProperty="userId" order="AFTER">
            SELECT LAST_INSERT_ID() AS user_id
        </selectKey>
        INSERT INTO ichecc_user_info(
				user_id,
				name,
				identity_card,
				address,
				province,
				city,
				country,
				create_time,
				modify_time
		)values(
				#{userId},
				#{name},
				#{identityCard},
				#{address},
				#{province},
				#{city},
				#{country},
				#{createTime},
				#{modifyTime}
		)
    </insert>
	

	<update id="MybatisIcheccUserInfoDAO_updateById" parameterType="com.ichecc.domain.IcheccUserInfoDO">
		UPDATE ichecc_user_info
		SET
					name = #{name},
					identity_card = #{identityCard},
					address = #{address},
					province = #{province},
					city = #{city},
					country = #{country},
					modify_time = #{modifyTime}
		WHERE
			user_id = #{userId}
	</update>

	<delete id="MybatisIcheccUserInfoDAO_deleteById" parameterType="long">
		DELETE FROM ichecc_user_info WHERE user_id = #{userId}
	</delete>
	
	
	<update id="MybatisIcheccUserInfoDAO_update_dynamic" parameterType="com.ichecc.domain.IcheccUserInfoDO">
		UPDATE ichecc_user_info
		<set>
					<if test="name != null  and name != '' ">name=#{name},</if>
					<if test="identityCard != null  and identityCard != '' ">identity_card=#{identityCard},</if>
					<if test="address != null  and address != '' ">address=#{address},</if>
					<if test="province != null  and province != '' ">province=#{province},</if>
					<if test="city != null  and city != '' ">city=#{city},</if>
					<if test="country != null  and country != '' ">country=#{country},</if>
					<if test="modifyTime != null ">modify_time=#{modifyTime},</if>
		</set>
		WHERE user_id = #{userId}
	</update>

 	<select id="MybatisIcheccUserInfoDAO_select_dynamic" resultMap="IcheccUserInfoDOResultMap" parameterType="com.ichecc.domain.IcheccUserInfoDO">
	     SELECT
	     	<include refid="MybatisIcheccUserInfoDAO_all_column_fields" />
		 FROM
		 	ichecc_user_info
			<include refid="MybatisIcheccUserInfoDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisIcheccUserInfoDAO_select_dynamic_count" resultType="long" parameterType="com.ichecc.domain.IcheccUserInfoDO">
	     SELECT
	     	count(1)
		 FROM
		 	ichecc_user_info
	 	<include refid="MybatisIcheccUserInfoDAO_dynamic_where_fields" />
 	</select>

 	<select id="MybatisIcheccUserInfoDAO_select_dynamic_page_query" resultMap="IcheccUserInfoDOResultMap" parameterType="com.ichecc.domain.IcheccUserInfoDO">
	     SELECT
	     	<include refid="MybatisIcheccUserInfoDAO_all_column_fields" />
		 FROM
		 	ichecc_user_info
	 	<include refid="MybatisIcheccUserInfoDAO_dynamic_where_fields" />
	 	ORDER BY ichecc_user_info.id DESC
	 	Limit #{start},#{pageSize}
 	</select>

</mapper>